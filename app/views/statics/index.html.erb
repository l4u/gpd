<div class="center">
 
  <div class="jumbotron">
    <h1>Welcome to Goalopedia! Discover Awesome Goals and Add Your Favorites</h1>
  </div>

	<div id="goals" class="masonry clearfix">
		<%= render @goals.order(:created_at).reverse_order %>
	</div>

	<div class="paginator">
  	<%= will_paginate @goals, renderer: BootstrapPagination::Rails %>
	</div>
	</div>

</div>